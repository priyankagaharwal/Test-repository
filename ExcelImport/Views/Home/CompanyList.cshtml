@model List<ExcelImport.Entities.CompanyDetail>
@{
    ViewBag.Title = "Home Page";
}

<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap.min.js"></script>
<link href="~/Content/DataTables/css/jquery.dataTables.min.css" rel="stylesheet" />
<script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>
<p><br /><br /></p>
<table id="empTable" class="table table-striped table-bordered dt-responsive nowrap dataTable no-footer dtr-inline collapsed" width="100%">
    <thead>
        <tr>
            <th>Company Name</th>
            <th>GSTIN</th>
            <th>Start Date</th>
            <th>End Date</th>
            <th>TurnOver Amount</th>
            <th>Email Id</th>
            <th>Contact No</th>
            <th class="all">&nbsp; &nbsp;&nbsp; Options &nbsp; </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(model => item.CompName)</td>
                <td>@Html.DisplayFor(model => item.GSTIN)</td>
                <td>@Html.DisplayFor(model => item.StartDate)</td>
                <td>@Html.DisplayFor(model => item.EndDate)</td>
                <td>@Html.DisplayFor(model => item.TurnOverAmount)</td>
                <td>@Html.DisplayFor(model => item.EmailId)</td>
                <td>@Html.DisplayFor(model => item.ContactNo)</td>
                <td class="all">
                    <button type="button" style="color:blue" compId="@item.CompId" class="editCompDetails">Edit</button>
                </td>
            </tr>
        }

    </tbody>
</table>




